                              "fontStretch": "normal",
                             "src": [ "file:./assets/fonts/raleway/Raleway-VariableFont_wght.ttf" ]
                          },
                          {
                                "fontFamily": "Raleway",
                                "fontWeight": "900",
                                "fontStyle": "normal",
                                "fontStretch": "normal",
                                "src": [ "file:./assets/fonts/raleway/Raleway-VariableFont_wght.ttf" ]
                          }
                      ]
                }

			]
		}
    },

	"styles": {
        
        "spacing": {
            "padding": {
                "left": "var(--wp--preset--spacing--contentgap)",
                "right": "var(--wp--preset--spacing--contentgap)"
            }
        },

        "blocks": {
            "core/site-title": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--secondary)",
                    "fontSize": "var(--wp--preset--font-size--font-54)",
                    "fontWeight": "600",
                    "lineHeight": "1.4"
                }
            },

            "core/post-title": {
                "color": {
                    "text": "var(--wp--preset--color--pine)"
                },
                "typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)",
                    "fontSize": "var(--wp--preset--font-size--font-54)",
                    "fontWeight": "600",
                    "lineHeight": "1.25",
                    "textTransform": "none"
                }
            },

            "core/button": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--secondary)",
                    "fontSize": "1.8rem",
                    "fontWeight": "600",
                    "lineHeight": "1.05"
                },
                "spacing": {
                    "padding": {
                        "top": "16px",
                        "right": "3rem",
                        "bottom": "16px",
                        "left": "6rem"
                    }
                },
                "color": {
                    "text": "var(--wp--preset--color--black)",
                    "background": "transparent"
                },
                "border": {
                    "color": "var( --wp--preset--color--secondary)",
                    "width": "1px",
                    "style": "solid",
                    "radius": "5rem"
                },
                
                "variations": {
                    "outline": {
                        "spacing": {
                            "padding": {
                                "top": ".85ch",
                                "right": "3rem",
                                "bottom": ".95ch",
                                "left": "3rem"
                            }
                        },
                         "color": {
                            "text": "var(--wp--preset--color--blue)",
                            "background": "transparent"
                        },
                        "border": {
                            "color": "var( --wp--preset--color--secondary)",
                            "width": ".15rem"
                        }
                    }
                }
            },

            "core/group": {
                "spacing": {
                    "padding": {
                        "top": "var(--wp--preset--spacing--xl)",
                        "bottom": "var(--wp--preset--spacing--xl)"
                    }
                }
            },

            "core/columns": {
                "spacing": {
                    "blockGap": "var(--wp--preset--spacing--normal)"
                }
            },

            "core/paragraph": {
                "typography": {
                    "lineHeight": "1.5",
                    "fontSize": "var(--wp--preset--font-size--font-19)",
                    "fontWeight": "400"
                },
                "spacing": {
                    "margin": {
                        "bottom": "1.5ch"
                    }
                }
            }
        },

        "color": {
            "background": "var(--wp--preset--color--white)",
            "text": "var(--wp--preset--color--primary)"
        },
        
        "elements": {
            "h1": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--secondary)",
                    "fontSize": "var(--wp--preset--font-size--font-60)",
                    "fontWeight": "600",
                    "lineHeight": "1.25",
                    "textTransform": "none"
                },
                "spacing": {
                    "margin": {
                        "bottom": ".8ch"
                    }
                },
                "color": {
                    "text": "var(--wp--preset--color--pine)"
                }
            },
        "h2": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--secondary)",
                "fontSize": "var(--wp--preset--font-size--font-54)",
                "fontWeight": "600",
                "lineHeight": "1.25"
            },
            "spacing": {
                "margin": {
                    "bottom": "1.1ch"
                }
            },
            "color": {
                "text": "var(--wp--preset--color--pine)"
            }
        },
        "h3": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--secondary)",
                "fontSize": "var(--wp--preset--font-size--font-32)",
                "fontWeight": "500",
                "lineHeight": "1.35"
            },
            "spacing": {
                "margin": {
                    "bottom": "1.15ch"
                }
            },
            "color": {
                "text": "var(--wp--preset--color--pine)"
            }
        },
        "h4": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--secondary)",
                "fontSize": "var(--wp--preset--font-size--font-26)",
                "fontWeight": "500",
                "lineHeight": "1.35"
            },
            "spacing": {
                "margin": {
                    "bottom": "1.25ch"
                }
            },
            "color": {
                "text": "var(--wp--preset--color--pine)"
            }
        },
        "h5": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--secondary)",
                "fontSize": "var(--wp--preset--font-size--font-22)",
                "fontWeight": "600",
                "lineHeight": "1.35"
            },
            "spacing": {
                "margin": {
                    "bottom": "1.5ch"
                }
            },
            "color": {
                "text": "var(--wp--preset--color--pine)"
            }
        },
        "h6": {
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--secondary)",
                "fontSize": "var(--wp--preset--font-size--font-20)",
                "fontWeight": "600",
                "lineHeight": "1.45",
                "textTransform": "none"
            },
            "spacing": {
                "margin": {
                    "bottom": "1.5ch"
                }
            },
            "color": {
                "text": "var(--wp--preset--color--pine)"
            }
        },

        "link": {
            "color": {
           		"text": "var(--wp--preset--color--primary)"
            },
            ":hover": {
				"color": {
					"text": "var(--wp--preset--color--purple)"
				}
            }
        }
        },

        "typography": {
            "fontFamily": "var(--wp--preset--font-family--primary)",
            "fontSize": "var(--wp--preset--font-size--font-18)",
            "lineHeight": "1.5"
        }
    }

}